    "version": "0.2.0",
    "configurations": [    
        {
            "name": "Launch Ela System",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceFolder}/internal/builds/linux/system",
            "program": "${workspaceFolder}/internal/cwd/system",
            "buildFlags": "-tags=IDE"
        },
        {
            "name": "Launch Installer",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "args": ["../../builds/linux/system/ela.system.box","-s","-l"],
            "program": "${workspaceFolder}/internal/cwd/packageinstaller",
            "buildFlags": "-tags=IDE"
        },
        {
            "name": "Launch Test Service",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/internal/cwd/servicetest",
        },
        {
            "name": "Launch Test Service Consumer",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/internal/cwd/servicetestconsumer",
        }
    ]
}